{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-zayyan-test"
		},
		"ADLSG2_to_ADF_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ADLSG2_to_ADF'"
		},
		"AdlsLookUpFiles_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AdlsLookUpFiles'"
		},
		"AzureBlobStorage1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobStorage1'"
		},
		"AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1'"
		},
		"Link_Blob_to_Blob_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Link_Blob_to_Blob'"
		},
		"Link_test_Blob_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Link_test_Blob'"
		},
		"LinkedServiceSqlServer_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LinkedServiceSqlServer'"
		},
		"LookupSqlDb_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LookupSqlDb'"
		},
		"LookupSqltoContainer_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'LookupSqltoContainer'"
		},
		"adf_to_SQL_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'adf_to_SQL'"
		},
		"ADLSG2_to_ADF_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://zayyanstoragetest2.dfs.core.windows.net"
		},
		"AdlsLookUpFiles_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://zayyanstoragetest2.dfs.core.windows.net"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://zayyanstoragetest2.dfs.core.windows.net"
		},
		"ScheduleEvery1hr_properties_ADLSForEach_parameters_folders": {
			"type": "array",
			"defaultValue": [
				"adls-fe-1",
				"adls-fe-2",
				"adls-fe-3"
			]
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-zayyan-test_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ADLSG2_to_ADF_accountKey": {
						"value": "[parameters('ADLSG2_to_ADF_accountKey')]"
					},
					"AdlsLookUpFiles_accountKey": {
						"value": "[parameters('AdlsLookUpFiles_accountKey')]"
					},
					"AzureBlobStorage1_connectionString": {
						"value": "[parameters('AzureBlobStorage1_connectionString')]"
					},
					"AzureDataLakeStorage1_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1_accountKey')]"
					},
					"Link_Blob_to_Blob_connectionString": {
						"value": "[parameters('Link_Blob_to_Blob_connectionString')]"
					},
					"Link_test_Blob_connectionString": {
						"value": "[parameters('Link_test_Blob_connectionString')]"
					},
					"LinkedServiceSqlServer_connectionString": {
						"value": "[parameters('LinkedServiceSqlServer_connectionString')]"
					},
					"LookupSqlDb_connectionString": {
						"value": "[parameters('LookupSqlDb_connectionString')]"
					},
					"LookupSqltoContainer_connectionString": {
						"value": "[parameters('LookupSqltoContainer_connectionString')]"
					},
					"adf_to_SQL_connectionString": {
						"value": "[parameters('adf_to_SQL_connectionString')]"
					},
					"ADLSG2_to_ADF_properties_typeProperties_url": {
						"value": "[parameters('ADLSG2_to_ADF_properties_typeProperties_url')]"
					},
					"AdlsLookUpFiles_properties_typeProperties_url": {
						"value": "[parameters('AdlsLookUpFiles_properties_typeProperties_url')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "adf-zayyan-test_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adf-zayyan-test_ArmTemplate_0')]"
			]
		},
		{
			"name": "adf-zayyan-test_ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_2.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ScheduleEvery1hr_properties_ADLSForEach_parameters_folders": {
						"value": "[parameters('ScheduleEvery1hr_properties_ADLSForEach_parameters_folders')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adf-zayyan-test_ArmTemplate_1')]"
			]
		},
		{
			"name": "adf-zayyan-test_ArmTemplate_3",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_3.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adf-zayyan-test_ArmTemplate_2')]"
			]
		}
	]
}